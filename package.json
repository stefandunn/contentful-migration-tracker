{
  "name": "contentful-migration",
  "version": "1.0.0",
  "main": "index.ts",
  "author": "Stefan Dunn <stefannicholasdunn@googlemail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "migrate": "node_modules/.bin/ts-node index.ts migrate",
    "migrate-dev": "CONTENTFUL_ENVIRONMENT_ID=development node_modules/.bin/ts-node index.ts migrate",
    "migrate-deploy": "yarn migrate-dev && yarn migrate && yarn build",
    "rollback": "node_modules/.bin/ts-node index.ts rollback",
    "rollback:to": "node_modules/.bin/ts-node index.ts rollback-to",
    "rollback:all": "node_modules/.bin/ts-node index.ts rollback-all",
    "make:migration": "node_modules/.bin/ts-node index.ts make-migration"
  },
  "dependencies": {
    "@contentful/rich-text-from-markdown": "^15.13.1",
    "@prisma/client": "^5.9.0",
    "@sentry/node": "^7.81.1",
    "@sentry/tracing": "^7.81.1",
    "@sentry/types": "^7.81.1",
    "contentful-migration": "^4.9.1",
    "dotenv": "^16.0.1",
    "firebase-admin": "^11.2.0",
    "kleur": "^4.1.4",
    "prisma": "^5.9.0",
    "uuid": "^9.0.0",
    "next": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.0.0"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "contentful": "^9.1.32",
    "contentful-cli": "^1.14.9",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5"
  }
}
