  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "runtimeVersion": "default",
      "name": "Migrate File",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/node_modules/.bin/ts-node",
      "args": [
        "index.ts",
        "migrate",
        "1657790878011_create_author_content_type"
      ],
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "type": "node",
      "request": "launch",
      "runtimeVersion": "default",
      "name": "Migrate All",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/node_modules/.bin/ts-node",
      "args": [
        "index.ts",
        "migrate"
      ],
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "type": "node",
      "request": "launch",
      "runtimeVersion": "default",
      "name": "Rollback last",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/node_modules/.bin/ts-node",
      "args": [
        "index.ts",
        "rollback"
      ],
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "type": "node",
      "request": "launch",
      "runtimeVersion": "default",
      "name": "Rollback all",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/node_modules/.bin/ts-node",
      "args": [
        "index.ts",
        "rollback-all"
      ],
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
